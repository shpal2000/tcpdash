    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch tlspack",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/tlspack/build/app_engine.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/tlspack",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build tlspack"
        }
    ]
}